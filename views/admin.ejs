<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <link href="/img/logo.png" rel="icon" /><link href="/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet" /><link href="/lib/animate/animate.min.css" rel="stylesheet" />
    <link href="/lib/ionicons/css/ionicons.min.css" rel="stylesheet" /><link href="/css/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="/css/signin.css">
    <link href="/css/dashboard.css" rel="stylesheet" />
    <script src="https://use.fontawesome.com/e2eea6562c.js"></script>
    <link href="/open-iconic/font/css/open-iconic.css" rel="stylesheet" />
    <title>Food2Fuel</title>
  </head>
  <body>
    <header id="header" style="box-shadow: 0px 2px 6px #888;">
      <div class="container-fluid">
        <div id="logo" class="pull-left pb-5">
          <h1 style=" display: inline;">
            <a style="color:white;" href="/" class="scrollto"
              >F<span
                ><img
                  height="35px;"
                  src="/img/logo.png"
                  alt="Hackathon"
                  title="Hackathon"/></span
              ><span
                ><img
                  height="35px;"
                  src="/img/logo.png"
                  alt="Hackathon"
                  title="Hackathon"/></span
              >d2Fuel</a
            >
          </h1>
        </div>

        <nav id="nav-menu-container">
          <ul class="nav-menu">
            <li><a href="/">Home</a></li>
          </ul>
        </nav>
      </div>
    </header>
    <div class="orderSection  " style="background: rgb(255, 255, 255); width: 65%; margin: 120px auto; box-shadow: 0px 0px 4px #888; ">
        <% for(let o of orders){ %>
        <div class="orderCard mx-2 py-2">
            <h3><strong> Quantity:</strong> <%= o.quantity %></h3>
            <h3><strong> Address:</strong><%= o.user.address %></h2>
            <h3><strong>Phone Number: </strong></strong><%= o.user.phone%></h3>
            <h3><strong>Restaurant Name: </strong></strong><%= o.user.name %></h3>
            <h3><strong>Date: </strong><%= o.user.date.toString().substring(0,11) %></h3>
            <button class="" style="border-radius: 0px; height: 40px; background: rgb(0, 250, 21); width: 130px; border: 0px solid black;"><a style="color: white;" href="/order/<%= o.id %>">Resolve</a></button>
        </div>
        <% } %>
    </div>
    <script src="/lib/jquery/jquery.min.js"></script>
    <script src="/lib/jquery/jquery-migrate.min.js"></script>
    <script src="/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/lib/easing/easing.min.js"></script>
    <script src="/lib/superfish/hoverIntent.js"></script>
    <script src="/lib/superfish/superfish.min.js"></script>
    <script src="/lib/wow/wow.min.js"></script>
    <script src="/lib/waypoints/waypoints.min.js"></script>
    <script src="/lib/counterup/counterup.min.js"></script>
    <script src="/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="/lib/lightbox/js/lightbox.min.js"></script>
    <script src="/lib/touchSwipe/jquery.touchSwipe.min.js"></script>
    <!-- Contact Form JavaScript File -->
    <script src="/js/contactform.js"></script>

    <!-- Template Main Javascript File -->
    <script src="/js/main.js"></script>
  </body>
</html>
