  <body>
    <header id="header" style="box-shadow: 0px 2px 6px #888;">
      <div class="container-fluid">
        <div id="logo" class="pull-left pb-5">
          <!-- Uncomment below if you prefer to use an image logo -->
          <!-- <a href="#intro"><img height="50px;" src="img/logo.png" alt="Hackathon" title="Hackathon"/></a>  -->
          <h1 style=" display: inline;">
            <a style="color:white;" href="#intro" class="scrollto"
              >F<span
                ><img
                  height="35px;"
                  src="/img/logo.png"
                  alt="Hackathon"
                  title="Hackathon"/></span
              ><span
                ><img
                  height="35px;"
                  src="/img/logo.png"
                  alt="Hackathon"
                  title="Hackathon"/></span
              >d2Fuel</a
            >
          </h1>
        </div>

        <nav id="nav-menu-container">
          <ul class="nav-menu">
            <li><a href="#intro">Home</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </nav>
      </div>
    </header>
    <div class="orderSection">
        <% for(let o of orders){ %>
        <div class="orderCard">
            <h3><%= o.quantity %></h3>
            <h3><%= o.user.address %></h2>
            <h3><%= o.user.phone%></h3>
            <h3><%= o.user.name %></h3>
            <h3><%= o.user.date %></h3>
            <a href="/order/<%= o.id %>">Click Me</a>
        </div>
        <% } %>
    </div>
